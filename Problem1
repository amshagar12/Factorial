import java.util.*;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);
		System.out.println("\n Enter The Number:");
		int n=sc.nextInt();
		long fact=1;
		
		if(n<0) {
			System.out.println("\n Error!");
		}
		
		else {
			for(long i=1; i<=n; i++) {
				fact*=i;}
				System.out.println("\n Factorial is: "+fact);
			
		}
		

	}

}
